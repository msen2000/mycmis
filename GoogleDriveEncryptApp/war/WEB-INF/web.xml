<?xml version="1.0" encoding="utf-8"?>
<web-app
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
    version="3.0">
	<servlet>
		<servlet-name>GoogleDriveEncryptApp</servlet-name>
		<servlet-class>org.sen.app.gooedriencryptapp.GoogleDriveEncryptAppServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GoogleDriveEncryptApp</servlet-name>
		<url-pattern>/googledriveencryptapp</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
